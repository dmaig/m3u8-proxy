{
  "version": 2,
  "builds": [
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/proxy/m3u8",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/proxy/ts",
      "dest": "/api/proxy.js"
    }
  ]
}
